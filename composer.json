{
    "name" : "joindin/joindin-api",
    "description" : "The API-backend that powers joind.in",
    "keywords" : ["joind.in", "events", "talks", "speaking", "feedback"],
    "homepage" : "https://api.joind.in",
    "license" : "",
    "support" : {
        "issues" : "https://joindin.jira.com",
        "irc" : "irc://freenet/#joindin",
        "source" : "https://github.com/joindin/joindin-api"
    },
    "require" : {
        "ext-pdo": "*",
        "ext-json": "*",
        "swiftmailer/swiftmailer": "^v5.4.9",
        "michelf/php-markdown": "^1.8.0",
        "guzzlehttp/guzzle": "^4.2.4",
        "guzzlehttp/oauth-subscriber": "0.1.*"
    },
    "require-dev": {
        "exussum12/coverage-checker": "^0.11.0",
        "jakub-onderka/php-parallel-lint": "^1.0",
        "phpunit/dbunit": "^2.0",
        "phpunit/phpunit": "^5.7",
        "roave/security-advisories": "dev-master",
        "squizlabs/php_codesniffer": "^3.4",
        "sensiolabs/security-checker": "^5.0"
    },
    "autoload" : {
        "psr-4" : {
            "Joindin\\Modifier\\" : "src/Modifier/"
        },
        "classmap" : [
            "src/controllers",
            "src/inc",
            "src/models",
            "src/routers",
            "src/services",
            "src/views"
        ]
    },
    "autoload-dev" : {
        "psr-4" : {
            "JoindinTest\\" : "tests/"
        },
        "classmap" : [
            "tests/controllers",
            "tests/inc",
            "tests/models",
            "tests/routers",
            "tests/views"
        ]
    },
    "config" : {
        "preferred-install": "dist",
        "platform" : {
            "php": "5.6.6"
        },
        "sort-packages" : true,
        "optimize-autoloader" : true
    }
}
